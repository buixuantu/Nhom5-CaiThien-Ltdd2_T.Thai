<?xml version="1.0" encoding="utf-8"?>

<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">

android:shape="rectangle">
        <solid android:color="#36F4EE"></solid>

        <corners android:bottomLeftRadius="40dp"
            android:topRightRadius="30dp"
            ></corners>
        <size
            android:width="150dp"
            android:height="50dp"></size>
    <gradient

        android:centerColor="#4D923E"
        android:endColor="#0F695F"
        android:startColor="#CA1B4D"></gradient>

    <stroke
        android:width="3dp"
        android:color="#03A9F4"
        android:dashWidth="10dp"
        android:dashGap="10dp"></stroke>




</shape>